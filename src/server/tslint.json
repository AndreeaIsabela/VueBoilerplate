{
  "rules": {
    "no-console": [true, "log", "error"],
    "triple-equals": true,
    "eofline": true,
    "indent": [true, "spaces"],
    "max-line-length": [true, 120],
    "no-trailing-whitespace": true,
    "class-name": true,
    "comment-format": [true, "check-space", "check-lowercase"],
    "interface-name": [true, "always-prefix"],
    "jsdoc-format": true,
    "new-parens": true,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": true,
    "one-line": [
      true,
      "check-catch",
      "check-finally",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "one-variable-per-declaration": true,
    "quotemark": [true, "single"],
    "semicolon": [true, "always", "ignore-interfaces"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  }
}
